% find en un vector
program testsort ; 

vars int arr[10], aux, i, search, res;

main () {
    for i = 1 to 10 do {
        aux = rand(1, 100);
        arr[i] = aux;
        write (arr[i]);
    }
    write('?');
    read(search);
    res = -1;
    for i = 1 to 10 do {
        if (arr[i] == search) then {
            res = i;
        }
    }
    write('e');
    write(res);
 }

