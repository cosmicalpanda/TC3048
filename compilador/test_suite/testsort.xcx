% sort de un vector
program testsort ; 

vars int arr[10], aux, i, j;

main () {
    aux = 10;
    for  i = 1 to 10 do {
        aux = aux - 1;
        arr[i] = aux;
        write (arr[i]);
    }
    write('s');
    for i = 1 to 10 do{
        for j = 1 to 10 - i do {
            if ( arr[j] > arr[ j+1 ] ) then {
                aux = arr[j+1];
                arr[j+1] = arr[j];
                arr[j] = aux;
            }
        }
    }
    write('e');
    for i = 1 to 10 do{
        write(arr[i]);
    }
 }

